{
  "name": "eduquest",
  "version": "1.0.0",
  "description": "EduQuest - Quiz Management Platform",
  "private": true,
  "workspaces": [
    "apps/web",
    "apps/mobile"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=apps/web",
    "dev:web": "npm run dev --workspace=apps/web",
    "dev:mobile": "npm run dev --workspace=apps/mobile",
    "build": "npm run build --workspace=apps/web && npm run build --workspace=apps/mobile",
    "build:web": "npm run build --workspace=apps/web",
    "build:mobile": "npm run build --workspace=apps/mobile",
    "lint": "npm run lint --workspaces",
    "preview": "npm run preview --workspace=apps/web"
  },
  "devDependencies": {}
}
